  // -------------------------------------------------

  // 3. 스크립트 src 기반 탐지

  // -------------------------------------------------

  const loadedScripts = Array.from(document.scripts)

    .map(s => s.src?.toLowerCase() || '')

    .filter(Boolean);

  const srcDetected = new Set();

  libraries.forEach(lib => {

    if (lib.src && loadedScripts.some(src => src.includes(lib.src))) {

      srcDetected.add(lib.name);

    }

  });

  // -------------------------------------------------

  // 4. DOM 기반 탐지

  // -------------------------------------------------

  const domDetected = new Set();

  libraries.forEach(lib => {

    if (lib.dom && document.querySelector(lib.dom)) {

      domDetected.add(lib.name);

    }

  });

  // -------------------------------------------------

  // 5. 전역 변수 기반 탐지

  // -------------------------------------------------

  const varDetected = new Set();

  libraries.forEach(lib => {

    if (lib.var && safeGet(lib.var)) {

      varDetected.add(lib.name);

    }

  });

  // -------------------------------------------------

  // 6. 최종 결과 수집

  // -------------------------------------------------

  const results = libraries.map(lib => {

    const detected = varDetected.has(lib.name) || domDetected.has(lib.name) || srcDetected.has(lib.name);

    const version = detected && lib.version ? lib.version() : (detected ? '확인됨' : '-');

    return {

      라이브러리: lib.name,

      상태: detected ? '사용 중' : '미사용',

      버전: version,

      출처: [

        varDetected.has(lib.name) ? '전역' : '',

        domDetected.has(lib.name) ? 'DOM' : '',

        srcDetected.has(lib.name) ? 'src' : ''

      ].filter(Boolean).join(', ') || '-'

    };

  });

  // -------------------------------------------------

  // 7. 결과 출력 (50개 스크립트와 동일 형식)

  // -------------------------------------------------

  const used = results.filter(r => r.상태 === '사용 중');

  const unused = results.filter(r => r.상태 === '미사용');

  console.log(`%c총 ${libraries.length}개 검사 → 사용 중: ${used.length}개`, 'color:#4caf50;font-weight:bold;font-size:14px');

  if (used.length > 0) {

    console.log('%c사용 중인 라이브러리', 'color:#4caf50;font-weight:bold');

    console.table(used);

  }

  if (unused.length > 0) {

    console.log(`%c미사용 라이브러리 (${unused.length}개)`, 'color:#9e9e9e;font-style:italic');

    console.table(unused.map(r => ({ 라이브러리: r.라이브러리 })));

  }

  // -------------------------------------------------

  // 8. 사용 중인 항목에 대한 상세 정보 출력

  // -------------------------------------------------

  if (used.length > 0) {

    console.log('%c사용 중인 라이브러리 상세 정보', 'color:#2196f3;font-weight:bold');

    libraries.filter(lib => used.some(u => u.라이브러리 === lib.name)).forEach(lib => {

      console.log(`%c${lib.name}`, 'color:#1976d2;font-weight:bold');

      try {

        console.table([lib.detail()]);

      } catch (e) {

        console.log('상세 정보 오류:', e.message);

      }

    });

  }

  // -------------------------------------------------
  // 9. 발견된 라이브러리 기반 기본 페이지 경로 제안 (100개 전부 매핑)
  // -------------------------------------------------
  if (used.length > 0) {
    console.log('%c발견된 라이브러리 기반 기본 페이지 경로 제안', 'color:#ff9800;font-weight:bold');
    const pathMap = {
      'jQuery': ['/admin', '/dashboard', '/login', '/panel'],
      'jQuery UI': ['/admin', '/dashboard', '/form', '/widgets'],
      'jQuery Migrate': ['/admin', '/dashboard'],
      'Bootstrap': ['/admin', '/panel', '/cpanel', '/dashboard'],
      'Popper.js': ['/admin', '/dashboard'],
      'Moment.js': ['/admin/calendar', '/schedule', '/events', '/reports'],
      'Day.js': ['/admin/calendar', '/schedule', '/events'],
      'Datepicker': ['/admin/form', '/booking', '/events'],
      'CKEditor 4': ['/admin/content', '/admin/post/edit', '/cms', '/editor'],
      'CKEditor 5': ['/admin/content', '/admin/post/edit', '/cms', '/editor'],
      'TinyMCE': ['/admin/content', '/admin/post/edit', '/cms', '/editor'],
      'Quill': ['/admin/content', '/admin/post/edit', '/cms'],
      'CodeMirror': ['/admin/code', '/editor', '/ide', '/dev'],
      'DataTables': ['/admin/users', '/admin/list', '/dashboard', '/reports'],
      'Select2': ['/admin/users', '/admin/settings', '/form', '/search'],
      'Chosen': ['/admin/users', '/form', '/settings'],
      'Parsley': ['/admin/form', '/register', '/contact'],
      'jQuery Validate': ['/admin/form', '/register', '/contact'],
      'Dropzone': ['/admin/upload', '/filemanager', '/media', '/gallery'],
      'Fileinput': ['/admin/upload', '/filemanager', '/media'],
      'Slick': ['/admin/gallery', '/slider', '/home', '/landing'],
      'Owl Carousel': ['/admin/gallery', '/slider', '/home'],
      'Swiper': ['/admin/gallery', '/slider', '/home', '/portfolio'],
      'Fancybox': ['/admin/gallery', '/media', '/photos', '/lightbox'],
      'Lightbox': ['/admin/gallery', '/media', '/photos'],
      'Chart.js': ['/admin/stats', '/dashboard/analytics', '/reports', '/metrics'],
      'FullCalendar': ['/admin/calendar', '/schedule', '/events', '/booking'],
      'SweetAlert': ['/admin', '/dashboard', '/settings'],
      'SweetAlert2': ['/admin', '/dashboard', '/settings', '/notifications'],
      'Toastr': ['/admin', '/dashboard', '/notifications', '/alerts'],
      'BlockUI': ['/admin', '/loading', '/dashboard'],
      'Lodash': ['/admin', '/dashboard', '/app'],
      'Underscore.js': ['/admin', '/dashboard', '/app'],
      'Axios': ['/admin/api', '/dashboard', '/app'],
      'Fetch API': ['/admin', '/dashboard'],
      'GSAP': ['/admin/animation', '/landing', '/home', '/demo'],
      'AdminLTE': ['/admin', '/dashboard', '/admin/index.html', '/panel'],
      'MetisMenu': ['/admin', '/sidebar', '/dashboard'],
      'iCheck': ['/admin/form', '/settings', '/profile'],
      'Bootstrap Switch': ['/admin/settings', '/toggle', '/preferences'],
      'Summernote': ['/admin/content', '/post/edit', '/cms', '/editor'],
      'Inputmask': ['/admin/form', '/register', '/profile'],
      'Daterangepicker': ['/admin/reports', '/analytics', '/filter'],
      'Bootstrap Colorpicker': ['/admin/settings', '/theme', '/customize'],
      'Bootstrap Tagsinput': ['/admin/tags', '/post/edit', '/categories'],
      'jQuery Knob': ['/admin/stats', '/dashboard', '/progress'],
      'Sparkline': ['/admin/stats', '/dashboard', '/metrics'],
      'Flot': ['/admin/charts', '/stats', '/analytics'],
      'Morris.js': ['/admin/charts', '/stats', '/reports'],
      'Raphaël': ['/admin/charts', '/vector', '/graphics'],
      'PACE': ['/admin', '/loading', '/dashboard'],
      'FastClick': ['/admin', '/mobile', '/dashboard'],
      'Vue.js': ['/admin', '/dashboard', '/app', '/panel'],
      'React': ['/admin', '/dashboard', '/app', '/panel'],
      'Angular': ['/admin', '/dashboard', '/app', '/panel'],
      'Alpine.js': ['/admin', '/dashboard', '/app'],
      'Backbone.js': ['/admin', '/app', '/dashboard'],
      'Ember.js': ['/admin', '/app', '/dashboard'],
      'Knockout.js': ['/admin', '/app', '/dashboard'],
      'Handlebars': ['/admin', '/templates', '/dashboard'],
      'Mustache': ['/admin', '/templates', '/dashboard'],
      'Laravel Echo': ['/admin', '/realtime', '/chat', '/notifications'],
      'Pusher': ['/admin', '/realtime', '/chat', '/live'],
      'Socket.IO Client': ['/admin', '/realtime', '/chat', '/live'],
      'SortableJS': ['/admin/list', '/dragdrop', '/kanban'],
      'Dragula': ['/admin/list', '/dragdrop', '/kanban'],
      'Hammer.js': ['/admin/touch', '/mobile', '/gestures'],
      'Lottie': ['/landing', '/home', '/animation', '/intro'],
      'Anime.js': ['/landing', '/home', '/animation', '/effects'],
      'Three.js': ['/3d', '/demo', '/visual', '/canvas'],
      'AOS': ['/landing', '/home', '/about', '/scroll'],
      'Tippy.js': ['/admin', '/tooltip', '/help', '/dashboard'],
      'Notyf': ['/admin', '/notifications', '/alerts'],
      'iziToast': ['/admin', '/notifications', '/alerts'],
      'Magnific Popup': ['/admin/gallery', '/media', '/lightbox'],
      'Photoswipe': ['/admin/gallery', '/media', '/photos'],
      'Masonry': ['/admin/gallery', '/grid', '/portfolio'],
      'Isotope': ['/admin/gallery', '/grid', '/filter'],
      'Monaco Editor': ['/admin/code', '/editor', '/ide', '/dev'],
      'Highlight.js': ['/docs', '/api', '/blog', '/code'],
      'Prism.js': ['/docs', '/api', '/blog', '/code'],
      'WordPress': ['/wp-admin', '/wp-login.php', '/admin'],
      'Vue Router': ['/admin', '/app', '/dashboard'],
      'Vuex': ['/admin', '/app', '/dashboard'],
      'Redux': ['/admin', '/app', '/dashboard'],
      'MobX': ['/admin', '/app', '/dashboard'],
      'Svelte': ['/admin', '/app', '/dashboard'],
      'Nuxt.js': ['/admin', '/dashboard', '/panel'],
      'Next.js': ['/admin', '/dashboard', '/panel'],
      'Gatsby': ['/admin', '/dashboard', '/panel'],
      'Quasar': ['/admin', '/dashboard', '/app'],
      'Vuetify': ['/admin', '/dashboard', '/panel'],
      'Element UI': ['/admin', '/dashboard', '/panel'],
      'Ant Design': ['/admin', '/dashboard', '/panel'],
      'Materialize': ['/admin', '/dashboard', '/panel'],
      'Bulma': ['/admin', '/dashboard', '/panel'],
      'Tailwind CSS': ['/admin', '/dashboard', '/panel'],
      'Foundation': ['/admin', '/dashboard', '/panel'],
      'Semantic UI': ['/admin', '/dashboard', '/panel'],
      'UIKit': ['/admin', '/dashboard', '/panel']
    };

    const suggested = new Set();
    used.forEach(u => {
      const paths = pathMap[u.라이브러리] || ['/admin', '/dashboard', '/login'];
      paths.forEach(p => suggested.add(p));
    });

    if (suggested.size === 0) {
      console.log('기본 경로 없음 → /admin, /login 시도 추천');
    } else {
      Array.from(suggested).sort().forEach(p => {
        console.log(`window.open('${p}') → ${location.origin}${p}`);
      });
    }
  }


  console.log('%c완전 탐지 완료! (100개 전부 검사됨)', 'color:#00bcd4;font-weight:bold;font-size:16px');

})(); 
